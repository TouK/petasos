{
  "only": [
    "src/**/*",
    "jest.setup.js"
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    }
  }
}
